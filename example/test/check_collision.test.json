{
    "$schema": "../../schema/testcase.schema.json",
    "target": {
        "label": "check_collision"
    },
    "cases": [
        {
            "name": "vector_addr = 4(Vx>0,Vy>0), pos < MAX : continue",
            "setup": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 4
                    },
                    {
                        "label": "pos",
                        "values": [
                            223,
                            223
                        ]
                    },
                    {
                        "label": "old_pos",
                        "values": [
                            222,
                            222
                        ]
                    }
                ]
            },
            "expected": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 4
                    }
                ]
            }
        },
        {
