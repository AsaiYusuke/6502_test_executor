{
    "$schema": "../../../schema/testcase.schema.json",
    "target": {
        "label": "control"
    },
    "cases": {
        "check the read/write count": {
            "setup": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 0
                    },
                    {
                        "label": "APU_PAD1",
                        "sequence": [
                            [
                                0,
                                1,
                                0,
                                0
                            ],
                            [
                                1,
                                1,
                                0
                            ]
                        ]
                    }
                ]
            },
            "expected": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "writeCount": 1
                    },
                    {
                        "label": "APU_PAD1",
                        "readCount": [
                            4,
                            3
                        ]
                    }
                ]
            }
        }
    }
}