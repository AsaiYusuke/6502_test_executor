{
    "$schema": "../../../schema/testcase.schema.json",
    "target": {
        "label": "check_collision"
    },
    "cases": {
        "flags of the Processor status register": {
            "setup": {
                "register": {
                    "P": {
                        "Negative": true,
                        "Overflow": true,
                        "Decimal": true,
                        "InterruptDisable": true,
                        "Break": true,
                        "Carry": false,
                        "Zero": true
                    }
                }
            },
            "expected": {
                "register": {
                    "P": {
                        "Negative": true,
                        "Overflow": true,
                        "Decimal": true,
                        "InterruptDisable": true,
                        "Break": true,
                        "Carry": true,
                        "Zero": false
                    }
                }
            }
        },
        "check only part of the flags": {
            "setup": {
                "register": {
                    "P": {
                        "Carry": false,
                        "Zero": true
                    }
                }
            },
            "expected": {
                "register": {
                    "P": {
                        "Carry": true,
                        "Zero": false
                    }
                }
            }
        }
    }
}