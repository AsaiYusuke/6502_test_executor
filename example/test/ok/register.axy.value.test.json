{
    "$schema": "../../../schema/testcase.schema.json",
    "target": {
        "start": {
            "label": "check_collision"
        }
    },
    "cases": {
        "decimal value": {
            "setup": {
                "register": {
                    "A": {
                        "value": 100
                    },
                    "X": {
                        "value": 100
                    },
                    "Y": {
                        "value": 100
                    }
                }
            },
            "expected": {
                "register": {
                    "A": {
                        "value": {
                            "eq": 223
                        }
                    },
                    "X": {
                        "value": {
                            "eq": 0
                        }
                    },
                    "Y": {
                        "value": {
                            "eq": 100
                        }
                    }
                }
            }
        },
        "hexadecimal value": {
            "setup": {
                "register": {
                    "A": {
                        "value": 100
                    },
                    "X": {
                        "value": 100
                    },
                    "Y": {
                        "value": 100
                    }
                }
            },
            "expected": {
                "register": {
                    "A": {
                        "value": {
                            "eq": "$DF"
                        }
                    },
                    "X": {
                        "value": {
                            "eq": "$0"
                        }
                    },
                    "Y": {
                        "value": {
                            "eq": "$64"
                        }
                    }
                }
            }
        },
        "binary value": {
            "setup": {
                "register": {
                    "A": {
                        "value": 100
                    },
                    "X": {
                        "value": 100
                    },
                    "Y": {
                        "value": 100
                    }
                }
            },
            "expected": {
                "register": {
                    "A": {
                        "value": {
                            "eq": "%11011111"
                        }
                    },
                    "X": {
                        "value": {
                            "eq": "%0"
                        }
                    },
                    "Y": {
                        "value": {
                            "eq": "%01100100"
                        }
                    }
                }
            }
        },
        "value of the address pointed to by label": {
            "setup": {
                "register": {
                    "Y": {
                        "value": {
                            "label": "vector_dic",
                            "offset": 4
                        }
                    }
                }
            },
            "expected": {
                "register": {
                    "Y": {
                        "value": {
                            "eq": 5
                        }
                    }
                }
            }
        },
        "value of the register": {
            "setup": {
                "register": {
                    "A": {
                        "value": 100
                    },
                    "X": {
                        "value": 100
                    },
                    "Y": {
                        "value": 223
                    }
                }
            },
            "expected": {
                "register": {
                    "Y": {
                        "value": {
                            "eq": {
                                "type": "register_a"
                            }
                        }
                    }
                }
            }
        }
    }
}