{
    "$schema": "../../../schema/testcase.schema.json",
    "target": {
        "label": "check_collision"
    },
    "cases": {
        "decimal value": {
            "setup": {
                "register": {
                    "A": 100,
                    "X": 100,
                    "Y": 100
                }
            },
            "expected": {
                "register": {
                    "A": {
                        "eq": 223
                    },
                    "X": {
                        "eq": 0
                    },
                    "Y": {
                        "eq": 100
                    }
                }
            }
        },
        "hexadecimal value": {
            "setup": {
                "register": {
                    "A": 100,
                    "X": 100,
                    "Y": 100
                }
            },
            "expected": {
                "register": {
                    "A": {
                        "eq": "$DF"
                    },
                    "X": {
                        "eq": "$0"
                    },
                    "Y": {
                        "eq": "$64"
                    }
                }
            }
        },
        "binary value": {
            "setup": {
                "register": {
                    "A": 100,
                    "X": 100,
                    "Y": 100
                }
            },
            "expected": {
                "register": {
                    "A": {
                        "eq": "%11011111"
                    },
                    "X": {
                        "eq": "%0"
                    },
                    "Y": {
                        "eq": "%01100100"
                    }
                }
            }
        },
        "value of the address pointed to by label": {
            "setup": {
                "register": {
                    "Y": {
                        "label": "vector_dic",
                        "offset": 4
                    }
                }
            },
            "expected": {
                "register": {
                    "Y": {
                        "eq": 5
                    }
                }
            }
        }
    }
}