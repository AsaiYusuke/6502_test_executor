{
    "$schema": "../../../schema/testcase.schema.json",
    "target": {
        "label": "check_collision"
    },
    "cases": {
        "customize configurations test": {
            "setup": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 4
                    }
                ]
            },
            "expected": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 4
                    }
                ]
            }
        }
    },
    "config": {
        "debugFile": "dist/example.dbg",
        "timeout": 10000,
        "invalidMemory": {
            "enable": true,
            "ignoreList": [
                {
                    "start": {
                        "address": "$100"
                    },
                    "size": "$FF"
                },
                {
                    "start": {
                        "address": "$2000"
                    },
                    "size": "$2020"
                },
                {
                    "detect": true
                },
                {
                    "name": "CHARS"
                }
            ]
        }
    }
}