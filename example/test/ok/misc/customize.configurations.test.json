{
    "$schema": "../../../schema/testcase.schema.json",
    "target": {
        "start": {
            "label": "check_collision"
        }
    },
    "testCases": {
        "customize configurations test": {
            "setup": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 4
                    }
                ]
            },
            "expected": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": {
                            "eq": 4
                        }
                    }
                ]
            }
        }
    },
    "config": {
        "debugFile": "dist/example.dbg",
        "maxCycleCount": "NES_NTSC_VSYNC",
        "testMemoryAccess": {
            "feature": [
                "WRITE_TO_READONLY_MEMORY",
                "ACCESS_TO_OUT_OF_SEGMENT"
            ],
            "ignoreList": [
                {
                    "start": {
                        "address": "$100"
                    },
                    "size": "$100"
                },
                {
                    "start": {
                        "address": "$2000"
                    },
                    "size": "$2020"
                },
                {
                    "detect": true
                },
                {
                    "name": "CHARS"
                }
            ]
        }
    }
}