{
    "$schema": "../../../schema/testcase.schema.json",
    "target": {
        "start": {
            "label": "check_collision"
        }
    },
    "testCases": {
        "contiguous memory area test": {
            "setup": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": [
                            4,
                            5
                        ]
                    }
                ]
            },
            "expected": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": [
                            {
                                "eq": 4
                            },
                            {
                                "eq": 5
                            }
                        ]
                    }
                ]
            }
        }
    }
}