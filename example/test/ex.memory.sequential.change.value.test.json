{
    "$schema": "../../schema/testcase.schema.json",
    "target": {
        "label": "control"
    },
    "cases": {
        "sequential change value test": {
            "setup": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 0
                    },
                    {
                        "label": "APU_PAD1",
                        "sequence": [
                            0,
                            1,
                            1,
                            0
                        ]
                    }
                ]
            },
            "expected": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 4
                    }
                ]
            }
        },
        "sequential change value test with contiguous memory area": {
            "setup": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 0
                    },
                    {
                        "label": "APU_PAD1",
                        "sequence": [
                            [
                                0,
                                1,
                                0,
                                0
                            ],
                            [
                                1,
                                1,
                                0
                            ]
                        ]
                    }
                ]
            },
            "expected": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 4
                    },
                    {
                        "label": "APU_PAD1",
                        "readCount": [
                            4,
                            3
                        ]
                    }
                ]
            }
        }
    }
}