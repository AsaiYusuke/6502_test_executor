{
    "$schema": "../../schema/testcase.schema.json",
    "target": {
        "label": "control"
    },
    "cases": {
        "setup:[vector_addr= 0], expected:[vector_addr=4]": {
            "setup": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 0
                    },
                    {
                        "label": "APU_PAD1",
                        "sequence": [
                            0,
                            1,
                            1,
                            0
                        ]
                    }
                ]
            },
            "expected": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 4
                    }
                ]
            }
        },
        "setup:[vector_addr=44], expected:[vector_addr=0]": {
            "setup": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 44
                    },
                    {
                        "label": "APU_PAD1",
                        "sequence": [
                            0,
                            1,
                            1,
                            0
                        ]
                    }
                ]
            },
            "expected": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 0
                    }
                ]
            }
        },
        "setup:[vector_addr= 0, PadInput=1off:2on -> 1on:2on -> 1off:2on -> 1off:2off], expected:[vector_addr=0]": {
            "setup": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 0
                    },
                    {
                        "label": "APU_PAD1",
                        "sequence": [
                            [
                                0,
                                1,
                                0,
                                0
                            ],
                            [
                                1,
                                1,
                                0
                            ]
                        ]
                    }
                ]
            },
            "expected": {
                "memory": [
                    {
                        "label": "vector_addr",
                        "value": 4
                    },
                    {
                        "label": "APU_PAD1",
                        "readCount": [
                            4,
                            3
                        ]
                    }
                ]
            }
        }
    }
}